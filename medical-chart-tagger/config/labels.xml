<?xml version="1.0" encoding="UTF-8"?>
<labels>
    <label>
        <id>MED</id>
        <name>Medication Error</name>
        <color>#FF0000</color>
        <shortcut>m</shortcut>
        <attributes>
            <attribute>
                <name>severity</name>
                <values>
                    <value>high</value>
                    <value>medium</value>
                    <value>low</value>
                </values>
            </attribute>
            <attribute>
                <name>error_type</name>
                <values>
                    <value>wrong_dosage</value>
                    <value>incorrect_medication</value>
                    <value>missing_prescription</value>
                </values>
            </attribute>
        </attributes>
    </label>
    
